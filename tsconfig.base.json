{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2015", "dom"],
    "jsx": "react",
    "module": "esnext",
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "outDir": "dist",
    "sourceMap": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "removeComments": true,
    "experimentalDecorators": true,
  }
}